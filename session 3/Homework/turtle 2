from turtle import *



color('grey')
for i in range(7):
    forward(100)
    left(52)

color('red')
for i in range (3):
    forward(100)
    left(120)

color('blue')
for i in range (4):
    forward(100)
    left(90)

color('brown')
for i in range (5):
    forward(100)
    lt(72)

color('yellow')
for i in range (6):
    forward (100)
    lt (60)
mainloop()